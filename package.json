{
  "name": "mine-performance-academy",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "crawl": "npx tsx scripts/crawl.ts",
    "extract-trainers": "npx tsx scripts/extract-trainers.ts",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start:test": "next start -p 3010",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "playwright test",
    "test:e2e": "playwright test",
    "validate": "npm run lint && npm run typecheck && npm run build",
    "prepush": "npm run validate && npm run test",
    "security:check": "node scripts/secret-scan.mjs && npm audit --audit-level=critical",
    "security:audit": "npm audit --audit-level=high",
    "security:secrets": "node scripts/secret-scan.mjs"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.460.0",
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "tailwind-merge": "^2.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "playwright": "^1.49.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  }
}
